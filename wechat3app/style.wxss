 .bgc2 .weui-uploader__input-box{ float: right; 
position:  relative;right: 0px;top: 0px;
margin-right:0px;
margin-bottom:0px;
width:28px;
height:28px;
border:1px solid #d9d9d9; 
 
}
.weui-uploader__input-box:before, .weui-uploader__input-box:after {
    content: " ";
    position: absolute;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background-color: #D9D9D9;
}

.bgc2 .weui-uploader__input-box:after {
    width: 20px;
    height: 2px;
}
 .bgc2 .weui-uploader__input-box:before {
    width: 2px;
    height: 20px;
}